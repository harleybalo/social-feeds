{
    "name": "harleybalo/social-bundle",
    "description": "Social Bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "require": {
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/var-dumper": "5.0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Harleybalo\\SocialBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Harleybalo\\SocialBundle\\Tests\\": "Tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}